[
  {
    "name": "UserLookup",
    "promptTemplate": "Fetch user data for ID {{input}}",
    "model": "UserDataAgent",
    "parameters": {
      "timeout": "30",
      "retries": "2"
    },
    "dataURL": "https://api.example.com/users",
    "headers": [
      {
        "key": "Authorization",
        "value": "Bearer YOUR_API_KEY"
      },
      {
        "key": "Accept",
        "value": "application/json"
      }
    ],
    "body": "{\"userId\": \"{{input}}\"}"
  }
]
